<div>
	<dl>
		<h2>My projects</h2>
			<% @my_assignments.each do |assign| %>
			<div>
				<dt><%= link_to assign.title, assignment_path(assign.id)%></dt>
				<dd>Task Date: <%=assign.date %></dd>
				<dd>Task Time: <%=assign.time %></dd>
			</div>
			<% end %>
	</dl>
</div>

<div>
	<h2> Matched Tasks</h2>
	<tr>
		<td>
			customer_id
		</td>
		<td>
			worker_id
		</td>
		<td>
			assignment.title
		</td>
	</tr>
</div>
