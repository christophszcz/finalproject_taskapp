	<h2> Matched Tasks as Worker </h2>
<table>
	<% @worker_assignments.each do |work| %>
	<tr>		
			<% if work.customer.nil? %>
			<td>Invalid Task <%= work.id %></td>
		<% else %>

			<td><%= link_to work.title, assignment_path(work.id) %></td>
			<td> </td>
			<td><%= work.worker.first_name %></td>
			<td><%= work.customer.first_name %></td>
<% end %>

	</tr>

<% end %>


<div class="field">
		<%= f.label :rating %>
		<%= f.select :rating, [['1'],['2'],['3'],['4'],['5']]%>
</div>
<div class="field">
		<%= f.label :Comment %>
		<%= f.text_area :comment %>
</div></br>